{
  "manifest_version": 3,
  "name": "Kagimori",
  "version": "1.0.7",
  "description": "TOTP/HOTP Password Manager",
  "icons": {
    "48": "icons/padlock-48.png",
    "96": "icons/padlock-96.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "downloads",
    "clipboardWrite"
  ],
  "action": {
    "default_icon": "icons/padlock.png",
    "default_title": "Kagimori",
    "default_popup": "static/main.html"
  }
}
