{
  "manifest_version": 2,
  "name": "Kagimori",
  "version": "1.0.2",
  "description": "TOTP/HOTP Password Manager",
  "icons": {
    "48": "icons/48/padlock.png",
    "96": "icons/96/padlock.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "downloads",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icons/2x/baseline_vpn_key_black_48dp.png",
    "default_title": "Kagimori",
    "default_popup": "static/main.html"
  }
}
